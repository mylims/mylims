{
  "name": "new-eln-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"vite\" \"npm run codegen-watch\"",
    "build": "vite build",
    "serve": "vite preview",
    "eslint": "eslint . --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "check-types": "tsc --noEmit",
    "tailwind:build": "tailwindcss build tailwind.css -o ./styles/tailwind.out.css",
    "components:update": "components-update -o src/components/tailwind-ui",
    "codegen-watch": "graphql-codegen --config codegen.yml --watch",
    "jscpd": "jscpd -l 10 -i \"**/__tests__/**\" -t 1 src",
    "playwright": "playwright test -c playwright.config.ts",
    "playwright-dev": "npm run playwright -- --debug --project=chromium"
  },

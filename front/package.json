    "@types/react-datepicker": "^3.1.8",
    "apollo-upload-client": "^15.0.0",
    "byte-size": "^7.0.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.22.1",
    "dialog-polyfill": "^0.5.6",
    "dialog-polyfill-universal": "^0.1.0",
    "formik": "^2.2.9",

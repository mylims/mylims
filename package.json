{
  "name": "mylims",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build",
    "check-types": "tsc --noEmit",
    "dev": "node ace serve --watch",
    "eslint": "eslint addons app commands config contracts mongodb providers start tests server.ts env.ts --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "postinstall": "node scripts/postinstall.mjs && npm run tailwind:build",
    "test": "npm run test-only && npm run eslint && npm run check-types",
    "test-only": "jest",
    "test-coverage": "jest --coverage",
    "tailwind:build": "tailwindcss build tailwind.css -o ./public/tailwind.out.css",
    "codegen": "graphql-codegen --config codegen.yml",
    "components:update": "components-update -o app/Components/tailwind-ui"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.0.1",
    "@graphql-codegen/cli": "1.21.3",
    "@graphql-codegen/typescript": "^1.21.1",
    "@graphql-codegen/typescript-resolvers": "^1.19.0",
    "@types/jest": "^26.0.22",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.14.39",
    "@types/proxy-addr": "^2.0.0",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/source-map-support": "^0.5.3",
    "@zakodium/components-update": "^1.4.2",
    "@zakodium/eslint-config": "^3.0.1",
    "@zakodium/tailwind-config": "^2.0.2",
    "adonis-jest": "^1.0.2",
    "autoprefixer": "^10.2.5",
    "eslint": "^7.24.0",
    "jest": "^26.6.3",
    "pino-pretty": "^4.7.1",
    "postcss": "^8.2.10",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.1.1",
    "typescript": "^4.2.4",
    "youch": "^2.2.1",
    "youch-terminal": "^1.1.0"
  },
  "dependencies": {
    "@adonisjs/auth": "^7.0.0",
    "@adonisjs/core": "^5.1.4",
    "@adonisjs/session": "^6.0.1",
    "@headlessui/react": "^1.0.0",
    "@lukeed/uuid": "^2.0.0",
    "@popperjs/core": "^2.9.2",
    "@types/react-datepicker": "^3.1.8",
    "@zakodium/adonis-mongodb": "^0.6.0",
    "adonis-apollo": "^0.13.1",
    "adonis-react": "^0.2.0",
    "base-x": "^3.0.8",
    "byte-size": "^7.0.1",
    "clsx": "^1.1.1",
    "date-fns": "^2.21.1",
    "dialog-polyfill-universal": "^0.1.0",
    "formik": "^2.2.6",
    "got": "^11.8.2",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.6",
    "ms": "^2.1.3",
    "phc-argon2": "^1.1.0",
    "proxy-addr": "^2.0.6",
    "react": "^17.0.2",
    "react-datepicker": "^3.7.0",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.3.2",
    "react-popper": "^2.2.5",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"

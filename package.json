{
  "name": "mylims",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "node ace build",
    "check-types": "tsc --noEmit",
    "dev": "node scripts/reset-dev.mjs & npm start",
    "eslint": "eslint . --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "postinstall": "node scripts/postinstall.mjs && npm run tailwind:build",
    "start": "node ace serve --watch",
    "test": "npm run test-only && npm run eslint && npm run check-types",
    "test-only": "jest",
    "test-coverage": "jest --coverage",
    "tailwind:build": "tailwindcss build tailwind.css -o ./public/tailwind.out.css",
    "codegen": "graphql-codegen --config codegen.yml",
    "components:update": "components-update -o app/Components/tailwind-ui"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^4.0.1",
    "@graphql-codegen/cli": "1.20.1",
    "@graphql-codegen/typescript": "^1.21.0",
    "@graphql-codegen/typescript-resolvers": "^1.18.2",
    "@types/jest": "^26.0.20",
    "@types/ms": "^0.7.31",
    "@types/node": "^14.14.31",
    "@types/proxy-addr": "^2.0.0",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "@types/source-map-support": "^0.5.3",
    "@zakodium/components-update": "^1.4.0",
    "@zakodium/eslint-config": "^3.0.0",
    "@zakodium/tailwind-config": "^2.0.1",
    "adonis-jest": "^1.0.2",
    "autoprefixer": "^10.2.4",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "pino-pretty": "^4.5.0",
    "postcss": "^8.2.6",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.0.3",
    "typescript": "^4.1.5",
    "youch": "^2.1.1",
    "youch-terminal": "^1.0.1"
  },
  "dependencies": {
    "@adonisjs/core": "^5.0.5-canary-rc-2",
    "@adonisjs/session": "^5.0.0",
    "@headlessui/react": "^0.3.1",
    "@lukeed/uuid": "^2.0.0",
    "@popperjs/core": "^2.8.0",
    "@zakodium/adonis-mongodb": "^0.3.6",
    "adonis-apollo": "^0.13.1",
    "adonis-react": "^0.2.0",
    "base-x": "^3.0.8",
    "byte-size": "^7.0.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.17.0",
    "formik": "^2.2.6",
    "got": "^11.8.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.6.4",
    "ms": "^2.1.3",
    "phc-argon2": "^1.1.0",
    "proxy-addr": "^2.0.6",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-dropzone": "^11.3.1",
    "react-popper": "^2.2.4",
    "reflect-metadata": "^0.1.13",
    "source-map-support": "^0.5.19"
  },
  "volta": {
    "node": "15.9.0",
    "npm": "7.5.4"
  }
}

{
  "name": "mylims",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"node ace serve --watch\" \"npm run codegen-watch\"",
    "build": "node ace build",
    "check-types": "tsc --noEmit",
    "eslint": "eslint addons app commands config contracts mongodb providers start tests server.ts env.ts --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepare": "node scripts/prepare.mjs && npm run tailwind:build",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest",
    "test-coverage": "jest --coverage",
    "tailwind:build": "tailwindcss build -i tailwind.css -o ./public/tailwind.out.css",
    "codegen-watch": "graphql-codegen --config codegen.yml --watch",
    "components:update": "components-update -o app/Components/tailwind-ui"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.3.7",
    "@graphql-codegen/cli": "2.2.0",
    "@graphql-codegen/typescript": "^2.2.2",
    "@graphql-codegen/typescript-resolvers": "^2.2.1",
    "@types/jest": "^27.0.2",
    "@types/ms": "^0.7.31",
    "@types/node": "^16.9.6",
    "@types/proxy-addr": "^2.0.0",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "@types/source-map-support": "^0.5.4",
    "@zakodium/components-update": "^1.4.3",
    "@zakodium/eslint-config": "^3.0.3",
    "@zakodium/tailwind-config": "^2.0.3",
    "adonis-jest": "^2.0.1",
    "autoprefixer": "^10.3.5",
    "concurrently": "^6.2.1",
    "eslint": "^7.32.0",
    "jest": "^27.2.1",
    "pino-pretty": "^7.0.1",
    "postcss": "^8.3.7",
    "prettier": "^2.4.1",
    "tailwindcss": "^2.2.15",
    "typescript": "^4.4.3",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.9",
    "@adonisjs/core": "^5.3.4",
    "@adonisjs/session": "^6.1.2",
    "@headlessui/react": "^1.4.1",
    "@heroicons/react": "^1.0.4",
    "@hookform/resolvers": "^2.8.0",
    "@lukeed/uuid": "^2.0.0",
    "@popperjs/core": "^2.10.1",
    "@slynova/flydrive": "^1.0.3",
    "@targos/adonis-drive": "^4.0.8",
    "@types/react-datepicker": "^4.1.7",
    "@zakodium/adonis-mongodb": "^0.11.0",
    "adonis-apollo": "^0.16.0",
    "adonis-datadrive": "^0.4.0",
    "adonis-react": "^0.2.1",
    "base-x": "^3.0.8",
    "byte-size": "^8.1.0",
    "clsx": "^1.1.1",
    "date-fns": "^2.24.0",
    "dialog-polyfill-universal": "^0.1.0",
    "formik": "^2.2.9",
    "got": "^11.8.2",
    "graphql-scalars": "^1.10.1",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^4.1.2",
    "ms": "^2.1.3",
    "phc-argon2": "^1.1.2",
    "proxy-addr": "^2.0.7",
    "react": "^17.0.2",
    "react-datepicker": "^4.2.1",

{
  "name": "mylims",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"node ace serve --watch\" \"npm run codegen-watch\"",
    "build": "node ace build --production",
    "check-types": "tsc --noEmit",
    "eslint": "eslint addons app commands config contracts mongodb providers start tests server.ts env.ts --cache",
    "eslint-fix": "npm run eslint -- --fix",
    "prepare": "node scripts/prepare.mjs && npm run tailwind:build",
    "prettier": "prettier --check .",
    "prettier-write": "prettier --write .",
    "test": "npm run test-only && npm run eslint && npm run prettier && npm run check-types",
    "test-only": "jest",
    "test-coverage": "jest --coverage",
    "tailwind:build": "tailwindcss build -i tailwind.css -o ./public/tailwind.out.css",
    "codegen-watch": "graphql-codegen --config codegen.yml --watch",
    "components:update": "components-update -o app/Components/tailwind-ui"
  },
  "devDependencies": {
    "@adonisjs/assembler": "^5.4.1",
    "@graphql-codegen/cli": "2.5.0",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-resolvers": "^2.5.0",
    "@types/jest": "^27.4.0",
    "@types/json-schema": "^7.0.9",
    "@types/ms": "^0.7.31",
    "@types/node": "^17.0.16",
    "@types/proxy-addr": "^2.0.0",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/source-map-support": "^0.5.4",
    "@zakodium/components-update": "^1.4.3",
    "@zakodium/eslint-config": "^3.0.3",
    "@zakodium/tailwind-config": "^3.1.0",
    "adonis-jest": "^2.0.1",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "eslint": "^7.32.0",
    "jest": "^27.5.1",
    "pino-pretty": "^7.5.1",
    "postcss": "^8.4.6",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.19",
    "typescript": "^4.5.5",
    "youch": "^2.2.2",
    "youch-terminal": "^1.1.1"
  },
  "dependencies": {
    "@adonisjs/auth": "^8.0.10",
    "@adonisjs/core": "^5.4.2",
    "@adonisjs/session": "^6.1.2",
    "@headlessui/react": "^1.4.3",
    "@heroicons/react": "^1.0.5",
    "@hookform/resolvers": "^2.8.8",
    "@lukeed/uuid": "^2.0.0",
    "@popperjs/core": "^2.11.2",
    "@slynova/flydrive": "^1.0.3",
    "@targos/adonis-drive": "^4.0.8",
    "@types/react-datepicker": "^4.3.4",
    "@zakodium/adonis-mongodb": "^0.13.0",
    "adonis-apollo": "^0.17.0",
    "adonis-datadrive": "^0.5.1",
    "adonis-react": "^0.2.1",
    "base-x": "^3.0.9",
    "byte-size": "^8.1.0",
    "clsx": "^1.1.1",
    "crypto-random-string": "^3.3.1",
    "date-fns": "^2.28.0",
    "dialog-polyfill-universal": "^0.1.0",
    "escape-string-regexp": "^4.0.0",
    "formik": "^2.2.9",
    "fs-synchronizer": "^0.2.1",

{
  "typescript": true,
  "commands": [
    "@adonisjs/core/build/commands",
    "@zakodium/adonis-mongodb/lib/commands",
    "./commands"
  ],
  "exceptionHandlerNamespace": "App/Exceptions/Handler",
  "aliases": {
    "App": "app",
    "Contracts": "contracts",
    "Config": "config"
  },
  "preloads": [
    "./start/routes",
    "./start/kernel",
    "./start/events"
  ],
  "providers": [
    "./providers/internal-user/UserProvider",
    "./providers/zakodium-auth/ZakodiumAuthProvider",
    "./providers/local-user/LocalProvider",
    "./addons/ldap/providers/LdapProvider",
    "@adonisjs/core",
    "@adonisjs/session",
    "@zakodium/adonis-mongodb",
    "@adonisjs/view",
    "adonis-apollo"
  ],
  "metaFiles": [
    {
      "pattern": "app/Schemas/*",
      "reloadServer": true
    },
    ".env",
    ".adonisrc.json",
    "resources/views/**/*.edge",
    "public/**"
  ]
}
